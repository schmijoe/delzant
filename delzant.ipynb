    "from delzant import *\n",
    "\n",
    "#Δ = Polygon(edges=[Edge([1,0],0),Edge([0,1],0),Edge([-1,-2],-1)])\n",
    "Δ = Polygon(points = [[0,0],[1,0],[0,1]])\n",
    "\n",
    "Δ.draw()\n",
    "display(Δ)\n",
    "p=Δ.edges[2].find_default_probe()\n",
    "M=np.block([[p],[-Δ.edges[2].λ[1],Δ.edges[2].λ[0]]]).transpose()\n",
    "display(M)\n",
    "display(det(M))\n"
   ]
